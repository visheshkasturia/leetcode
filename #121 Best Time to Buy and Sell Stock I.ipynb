    "def maxProfit(prices):\n",
    "    \n",
    "    maxP=0\n",
    "    si=0\n",
    "    i=1\n",
    "    while i <len(prices):\n",
    "        p=prices[i]-prices[si]\n",
    "        if p<0:\n",
    "            si=i\n",
    "        else:\n",
    "            maxP=max(maxP,p)\n",
    "        i+=1\n",
    "\n",
    "    return maxP\n",
    "\n",
    "prices=[7,8,5,3,6,1,4,10]\n",
    "print(maxProfit(prices))\n",
    "    \n"
   ]
  }
