    "def reverseList(li):\n",
    "    si=0\n",
    "    ei=len(li)-1\n",
    "    index(li,si,ei)\n",
    "    return li\n",
    "    \n",
    "def index(li,si,ei):\n",
    "    if si>=ei:\n",
    "        return li\n",
    "    else:\n",
    "        li[si],li[ei]=li[ei],li[si]\n",
    "        return index(li,si+1,ei-1)\n",
    "\n",
    "    \n",
    "\n",
    "li=[\"h\",\"e\",\"l\",'l','o']\n",
    "reverseList(li)\n",
    "print(li)"
   ]
  }
